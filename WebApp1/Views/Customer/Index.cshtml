@model List<Customer>

    
    
    


<a asp-controller="Customer" asp-action="Create" class="btn btn-success" >Create</a> <br/>
<table class="table table-hover">
    <tbody>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age Name</th>
    </tr>
    </tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Age</td>
            <td>
                <a asp-controller="Customer" asp-action="Update" asp-route-id="@item.Id" class="btn btn-primary">Update</a>
                <a asp-controller="Customer" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>




